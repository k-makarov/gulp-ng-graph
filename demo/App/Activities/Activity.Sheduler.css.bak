/* << Кастомизация */
/* background color for whole container and it's border*/
.event {
    background-color: #add8e6;
    border: 1px solid #778899;
    overflow: hidden;
}
/* disabling default color for select menu */
.dhx_cal_select_menu.event div {
    border: 0;
    background-color: transparent;
    color: black;
}
/* styles for event content */
.dhx_cal_event.event .event_body {
    padding-top: 3px;
    padding-left: 5px;
}
/* event's date information */
.event .event_date {
    font-weight: bold;
    padding-right: 5px;
}
/* event's resizing section */
.event_resize {
    height: 3px;
    position: absolute;
    bottom: -1px;
}
/* event's move section */
.event_move {
    position: absolute;
    top: 0;
    height: 5px;
    cursor: pointer;
    background-color:transparent !important;
    color:transparent !important;
}
