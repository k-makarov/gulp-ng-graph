/* Замена стиля Flat */
.dhx_cal_container { font-size:inherit;  font-family:inherit;}

.dhx_section_time input[type=text] {
    font-size: 12px;
}

.dhx_section_time select {
    border: 1px solid #ccc;
    font-family: inherit;
    box-sizing: border-box;
    font-size: 12px;
    border-radius: 5px;
    background: #F2F2F2;
    width:60px;
    -webkit-appearance: none;
    background-image: url('../../../Content/Images//grid-sort-btn.png');
    background-position: right center;
    background-repeat: no-repeat;
    line-height: 1em;
    /* for FF */
    -moz-appearance: none;
    text-indent: 0.01px; 
    text-overflow: '';
    /* for IE */
    -ms-appearance: none;
    appearance: none!important;
}
.dhx_section_time select::-ms-expand {
  display: none;
}
.dhx_section_time option {
    font-family: inherit;
    font-size: 15px;
}


